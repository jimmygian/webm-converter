{
  "name": "fa-webm-convertor",
  "version": "1.0.0",
  "license": "MIT",
  "description": "An app that converts files to WEBM format",
  "author": "Dimitris Giannoulis",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "files": [
      "src/**/*"
    ],
    "appId": "com.example.fa-webm-convertor",
    "mac": {
      "category": "public.app-category.video",
      "target": ["dmg"]
    }
  }
}
